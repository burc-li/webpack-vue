/**
 * Vue支持JSX语法
 * npm i babel-preset-env babel-plugin-transform-vue-jsx
 */

{
  "presets": ["@babel/preset-env"],
  "plugins": [ 
    "transform-vue-jsx",
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ],
    "syntax-dynamic-import"
  ]
}

